{
	"java": {
		"install_flavor": "oracle", 
		"jdk_version": "7", 
		"oracle": { 
			"accept_oracle_download_terms": true
		}
	},
	"chef-server": {
		"configuration": {
			"nginx": {
				"non_ssl_port": 9090,
				"ssl_port": 9191 
			}, 
			"chef_server_webui": {
				"enable": true 
			}
		},
		"package_file": "https://opscode-omnibus-packages.s3.amazonaws.com/el/6/x86_64/chef-server-11.1.6-1.el6.x86_64.rpm"
	},
	"run_list": [
		"recipe[tar]",
		"recipe[git]",
		"recipe[java::default]",
		"recipe[chef-server::default]"
	]
}
