{
	"java": {
		"install_flavor": "oracle", 
		"jdk_version": "7", 
		"oracle": { 
			"accept_oracle_download_terms": true
		}
	},
	"jenkins": {
		"master": {
			"install_method": "war",
			"version": "1.555",
			"jvm_options": null,
			"jenkins_args": null,
			"user": "jenkins",
			"group": "jenkins",
			"use_system_accounts": true,
			"host": "localhost",
			"listen_address": "0.0.0.0",
			"port": 8080,
			"home": "/var/lib/jenkins",
			"log_directory": "/var/log/jenkins",
			"runit": {
				"sv_timeout": 7
			}			
		}
	},
	"run_list": [
		"recipe[tar]",
		"recipe[java::default]",
		"recipe[jenkins::master]"
	]
}